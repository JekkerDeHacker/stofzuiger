int US=A0;
int UTRASOUNDSERVO=13;
 
int CheckRock(int PinLoc){
  if(digitalRead(PinLoc) ==0){
    return 0;
  }else{ return 1;
}
}

#include<Servo.h>

void setup()
{
  Serial.begin(9600)
  pinMode(US, INPUT);
  pinMode(LEFTSERVO, OUTPUT);
  pinMode(RIGHTSERVO, OUTPUT);
}

void loop()
{
  if(USmeasure() >= 10) // Forward
  {Forward}
  else if(USmeasure() <= 10)// Turn Left
  {BackLeft}
}
